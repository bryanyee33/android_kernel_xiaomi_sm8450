# Arch
CONFIG_ARM64_SVE=n
CONFIG_ARM64_SW_TTBR0_PAN=n
CONFIG_UNMAP_KERNEL_AT_EL0=n

# Binder
CONFIG_BINDER_PRIO=m

# Boot
CONFIG_CMDLINE="stack_depot_disable=on kasan.stacktrace=off kvm-arm.mode=protected cgroup_disable=pressure cgroup.memory=nokmem noirqdebug quiet"

# Compiler
CONFIG_CC_OPTIMIZE_FOR_PERFORMANCE_O3=y
CONFIG_LLVM_POLLY=y

# Coresight
CONFIG_CORESIGHT=n
CONFIG_STM=n

# EDAC
CONFIG_EDAC=n

# FS
CONFIG_NTFS_FS=y
CONFIG_NTFS_RW=y

# IO scheduler
CONFIG_IOSCHED_BFQ=n
CONFIG_MQ_IOSCHED_KYBER=n
CONFIG_MQ_IOSCHED_SSG=y
CONFIG_MQ_IOSCHED_SSG_CGROUP=y

# Kfence
CONFIG_KFENCE=n

# MM
CONFIG_LRU_GEN_ENABLED=y

# Net
CONFIG_NET_SCH_DEFAULT=y
CONFIG_DEFAULT_FQ_CODEL=y

# Power
CONFIG_WQ_POWER_EFFICIENT_DEFAULT=y

# Version
CONFIG_LOCALVERSION="-aospa"
CONFIG_UNAME_OVERRIDE=y
CONFIG_UNAME_OVERRIDE_TARGET="com.google.android.gms"
CONFIG_UNAME_OVERRIDE_STRING="5.10.198-android12-9-00085-g226a9632f13d-ab11136126"
